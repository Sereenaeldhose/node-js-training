// With functional brackets. If no arguments, parentheses are empty, but they must be present.
hello = () => {
  return "Hello World!";
} 
console.log(hello());

// With parameter passing, If only parameter is passed then no need of ().
let greek1 = x => console.log(x);
greek1("Hello World!");

// having multiple parameter to pass. If function have multile statement then enclose with in {}.
let sum = (a, b) => {
  let result = a + b;
  return result;
}
console.log(sum(5,7));

// having multiple parameter to pass. with single statement.
let sum1 = (a, b) => a + b;
console.log(sum1(5,7));
